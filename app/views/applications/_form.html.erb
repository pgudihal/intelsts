<%= semantic_form_for @application do |f| %>

   <%= f.inputs do %>
   <%= f.input :application_name, :label => 'Application Name'%>
    <%= f.input :field_name, :label => 'Field'%>
    <%= f.input :filename, :label => 'File Name' %>
    <%= f.input :applicant, :label => 'Applicant', :required => true %>
  <%end %>

  <%= f.buttons do %>
  <%= f.commit_button %>
  <% end %>
<% end %>
